class LetterT{
    public static void main(String[] Args){
        for(int i=1;i<=11;i++){
            if(i==1){
                for(int j=1;j<=21;j++){
                    System.out.print("*");
                }
            }
            else{
                for(int j=1;j<=10;j++){
                    System.out.print(" ");
                }
                System.out.print("*");
            }
            System.out.println();
        }
    }
}

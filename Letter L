class LetterL{
    public static void main(String[] args){
        for(int i=1;i<11;i++){
            System.out.print("*");
            if(i==10){
                for(int j=1;j<=11;j++){
                    System.out.print("*");
                }
            }
            System.out.println();
        }
    }
}

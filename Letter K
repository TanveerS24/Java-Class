class LetterK{
    public static void main(String[] args){
        for(int i=1;i<=11;i++){
            System.out.print("*");
            if(i<=6){
                for(int j=6;j>=i;j--){
                    System.out.print(" ");
                }
            }
            else{
                for(int j=1;j<=i-5;j++){
                    System.out.print(" ");
                }
            }
            System.out.println("*");
        }
    }
}

class Letters{
    public static void main(String[] args){
        for(int i=1;i<=5;i++){
            for(int j=1;j<=5;j++){
                if(j<i){
                    System.out.print("  ");
                }
                else{
                    int a=70-j;
                    char x=(char)a;
                    System.out.print(x+" ");
                }
            }
            System.out.println();
        }
    }
}

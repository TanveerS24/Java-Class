class LetterW{
    public static void main(String[] Args){
        for(int i=1;i<=10;i++){
            for(int j=1;j<=40;j++){
                if(i==j || i+j==20 || i+j==40 || j-i==20){
                    System.out.print("*");
                }
                else{
                    System.out.print(" ");
                }
            }
            System.out.println();
        }
    }
}

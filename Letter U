class LetterU{
    public static void main(String[] Args){
        for(int i=1;i<=10;i++){
            if(i==10){
                System.out.print(" ");
                for(int j=1;j<=8;j++){
                    System.out.print("*");
                }
            }
            else{
                for(int j=1;j<=10;j++){
                    if(j==1 || j==10){
                        System.out.print("*");
                    }
                    else{
                        System.out.print(" ");
                    }
                }
            }
            System.out.println();
        }
    }
}

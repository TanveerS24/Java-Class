//Letter E
class LetterE{
    public static void main(String[] args){
        for(int i=1;i<=10;i++){
            if(i==1 || i==5 ||i==10){
                for(int j=1;j<=10;j++){
                    System.out.print("*");
                }
            }
            else{
                System.out.print("*");
            }
            System.out.println();
        }    
    }
}
